{
  "name": "feathers-4-boilerplate",
  "description": "A lightweight boliler plate for feathers",
  "version": "0.0.1",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Jonathan Crockett",
    "email": "joncrockett@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "config": "config/"
  },
  "engines": {
    "node": "^20.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run compile && npm run jest",
    "lint": "eslint src/. --config .eslintrc.json --ext .ts --fix",
    "dev": "ts-node-dev --no-notify src/",
    "start": "npm run compile && node lib/",
    "jest": "NODE_ENV=test node -expose-gc ./node_modules/.bin/jest --logHeapUsage --detectOpenHandles --runInBand",
    "compile": "shx rm -rf lib/ && tsc",
    "db:wipe:dev": "NODE_ENV=development ts-node-dev --exit-child './src/scripts/wipe-db.ts'",
    "db:wipe:test": "NODE_ENV=test ts-node-dev --exit-child './src/scripts/wipe-db.ts'"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
    "@feathersjs/configuration": "^4.5.11",
    "@feathersjs/errors": "^4.5.11",
    "@feathersjs/express": "^4.5.11",
    "@feathersjs/feathers": "^4.5.11",
    "@feathersjs/socketio": "^4.5.11",
    "@feathersjs/transport-commons": "^4.5.11",
    "@types/feathersjs__feathers": "^3.1.8",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-hooks-common": "^5.0.6",
    "feathers-memory": "^4.1.0",
    "feathers-sequelize": "^6.4.0",
    "helmet": "^5.1.1",
    "moment": "^2.29.4",
    "optimist": "^0.6.1",
    "prompt": "^1.3.0",
    "sequelize": "^6.35.1",
    "serve-favicon": "^2.5.0",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bluebird": "^3.5.42",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.10",
    "@types/serve-favicon": "^2.5.7",
    "@types/validator": "^13.11.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "3.1.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.1.3"
  }
}